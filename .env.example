# Example .env file for configuration
# Copy this to .env and customize as needed

# Control Plane Configuration
CONTROL_PLANE_PORT=8000
LOG_LEVEL=INFO

# Server Configuration
SERVER_BASE_PORT=8001
SERVER_COUNT=50

# Resource Limits (per server)
SERVER_CPU_LIMIT=0.5
SERVER_MEMORY_LIMIT=256M
SERVER_CPU_RESERVATION=0.1
SERVER_MEMORY_RESERVATION=128M

# Auto-refresh settings
AUTO_REFRESH_INTERVAL=5000  # milliseconds

# Load Test Defaults
DEFAULT_REQUESTS=1000
DEFAULT_CONCURRENCY=10
